{
  "manifest_version": 2,

  "name": "Messing with Event pages",
  "description": "This extension does X",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "alarms",
    "notifications",
    "https://pilotweb.nas.faa.gov/"
  ]
}
